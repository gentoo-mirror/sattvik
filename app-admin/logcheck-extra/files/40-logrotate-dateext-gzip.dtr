#!/usr/bin/perl

sub {
  my ($filename) = @_;
  my $rotated_filename="";
  # find out whether we have a dateext rotation scheme
  my @list = glob("$filename-[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]\.gz");
  if ( @list ) {
    $rotated_filename = (sort(glob("$filename-[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]\.gz")))[-1];
  }
  return $rotated_filename;
}
